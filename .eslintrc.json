{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-alert":"off",
        "linebreak-style": ["error", "windows"],
        "no-unused-vars":"off",
        "one-var":"off",
        "no-var":"off",
        "no-underscore-dangle":"off"
    }
}
